title = {{Effects of high intensity ultrasound on disaggregation of a macromolecular procyanidin-rich fraction from Vitis vinifera L. seed extract and evaluation of its antioxidant activity}},
url = {https://www.sciencedirect.com/science/article/abs/pii/S1350417718310617},
volume = {50},
year = {2019}
}
