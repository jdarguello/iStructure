journal = {Renewable and Sustainable Energy Reviews},
keywords = {Binders,Biomass,Pellets,Steam explosion,Torrefaction},
number = {January},
pages = {332--348},
publisher = {Elsevier Ltd},
title = {{The significance of pelletization operating conditions: An analysis of physical and mechanical characteristics as well as energy consumption of biomass pellets}},
